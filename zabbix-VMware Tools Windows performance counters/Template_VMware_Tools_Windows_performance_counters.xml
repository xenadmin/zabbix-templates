                    <triggers>
                        <trigger>
                            <expression>{last(5m)}&gt;0</expression>
                            <name>VM Memory Ballooning auf {HOST.NAME} (MB{ITEM.LASTVALUE})</name>
                            <opdata>Auf der virtuellen Windowsmachine {HOST.NAME} wurde VM Memory Ballooning festgestellt. Dies deutet auf einen Memoryengpass des VMware Hostsystems hin.</opdata>
                            <priority>WARNING</priority>
                            <description>Indiz für Ressourcenmangel im Memorybereich.</description>
                            <dependencies>
                                <dependency>
                                    <name>VM Memory Swapping auf {HOST.NAME} (MB{ITEM.LASTVALUE})</name>
                                    <expression>{CS3 - Virtual Windows Machines:perf_counter_en[\VM Memory\Memory Swapped in MB].last(5m)}&gt;0</expression>
                                </dependency>
                            </dependencies>
