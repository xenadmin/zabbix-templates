    <templates>
        <template>
            <template>CS3 - Virtual Windows Machines</template>
            <name>CS3 - Virtual Windows Machines</name>
            <description>Bunch of performance/systemcounter for virtual windows machines</description>
            <groups>
                <group>
                    <name>CS3-Templates</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>VM Tools Performance</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>VM Memory - Memory Free in MB</name>
                    <type>CALCULATED</type>
                    <key>cs3.windows.vmtools.memory.free</key>
                    <history>7d</history>
                    <trends>180d</trends>
                    <units>B</units>
                    <params>last(&quot;perf_counter_en[\VM Memory\Memory Limit in MB]&quot;) - last(&quot;perf_counter_en[\VM Memory\Memory Used in MB]&quot;)</params>
                    <applications>
                        <application>
                            <name>VM Tools Performance</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>VM Memory - Memory Active in MB</name>
                    <key>perf_counter_en[\VM Memory\Memory Active in MB]</key>
                    <history>7d</history>
                    <trends>180d</trends>
                    <units>B</units>
                    <applications>
                        <application>
                            <name>VM Tools Performance</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>MULTIPLIER</type>
                            <params>1048576</params>
                        </step>
                    </preprocessing>
                </item>
                <item>
                    <name>VM Memory - Memory Ballooned in MB</name>
